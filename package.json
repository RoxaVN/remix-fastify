    "eslint": "8.28.0",
    "eslint-plugin-prefer-let": "3.0.1",
    "estrella": "^1.4.1",
    "fs-extra": "^11.1.0",
    "glob": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "semver": "^7.3.8",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
